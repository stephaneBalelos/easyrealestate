    "styles": {
        "color": {
            "background": "var:preset|color|easyrealestate-bg",
            "text": "var:preset|color|easyrealestate-text"
        },
        "blocks": {
            "core/button": {
                "border": {
                    "color": null,
                    "radius": "var:custom|border-radius|medium",
                    "style": null,
                    "width": null
                },
                "spacing": {
                    "padding": {
                        "bottom": "calc(1rem - 1px)",
                        "left": "calc(2.25rem - 1px)",
                        "right": "calc(2.25rem - 1px)",
                        "top": "calc(1rem - 1px)"
                    }
                },
                "variations": {
                    "outline": {
                        "color": {
                            "background": "transparent",
                            "text": "currentColor"
                        },
                        "border": {
                            "color": "currentColor",
                            "width": "2px"
                        },
                        "css": ".wp-block-button__link:not(.has-background):hover {background-color:color-mix(in srgb, currentColor 5%, transparent);}",
                        "spacing": {
                            "padding": {
                                "bottom": "calc(1rem - 1px)",
                                "left": "calc(2.25rem - 1px)",
                                "right": "calc(2.25rem - 1px)",
                                "top": "calc(1rem - 1px)"
                            }
                        }
                    }
                }
            }
        },
        "spacing": {
            "blockGap": "16px",
            "padding": {
                "bottom": "0px",
                "left": "0px",
                "right": "0px",
                "top": "0px"
            }
        },
        "elements": {
            "h1": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--xx-large)",
                    "fontWeight": "900",
                    "lineHeight": "1.2"
                }
            },
            "h2": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--x-large)",
                    "fontWeight": "700",
                    "lineHeight": "1.35"
                }
            },
            "h3": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--large)",
                    "fontWeight": "700",
                    "lineHeight": "1.35"
                }
            },
            "h4": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--medium)",
                    "fontWeight": "700",
                    "lineHeight": "1.4"
                }
            },
            "h5": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--normal)",
                    "fontWeight": "700",
                    "lineHeight": "1.55"
                }
            },
            "h6": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--small)",
                    "fontWeight": "700",
                    "lineHeight": "1.6"
                }
            },
            "heading": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--nunitosans)"
                },
                "spacing": {
                    "margin": {
                        "bottom": ".5rem",
                        "top": "0"
                    }
                }
            },
            "button": {
                "color": {
                    "background": "var:preset|color|easyrealestate-primary",
                    "text": "var:preset|color|easyrealestate-bg"
                },
                ":focus": {
                    "outline": {
                        "color": "var:preset|color|easyrealestate-primary-dark",
                        "offset": "2px"
                    }
                },
                ":hover": {
                    "color": {
                        "background": "color-mix(in srgb, var(--wp--preset--color--easyrealestate-primary) 85%, transparent)",
                        "text": "var:preset|color|easyrealestate-bg"
                    },
                    "border": {
                        "color": "transparent"
                    }
                },
                "spacing": {
                    "padding": {
                        "bottom": ".5rem",
                        "left": "1.5rem",
                        "right": "1.5rem",
                        "top": ".5rem"
                    }
                },
                "typography": {
                    "fontSize": "var:preset|font-size|medium",
                    "fontWeight": "700"
                }
            }
        },
        "typography": {
            "fontFamily": "var(--wp--preset--font-family--nunitosans)",
            "fontSize": "var(--wp--preset--font-size--normal)",
            "fontWeight": "400",
            "lineHeight": "1.3"
